{
  "expo": {
    "name": "Aldo Tweets Challenge",
    "slug": "aldo-tweets-challenge",
    "description": "A React Native app to fetch latest tweets of Donald Trump and Hillary Clinton",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/twitter.png",
    "splash": {
      "image": "./assets/twittersplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.thescriptworm.aldotweetschallenge",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.thescriptworm.aldotweetschallenge",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
